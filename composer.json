{
  "name": "tudomakerspace/ns-theme-cleanblog",
  "type": "typo3-cms-extension",
  "description": "The TU-DO Makerspace's fork of NITSAN's Clean Blog theme",
  "keywords": [
    "TYPO3 CMS",
    "blog",
    "bootstrap",
    "responsive",
    "frontend",
    "theme",
    "web"
  ],
  "homepage": "https://github.com/TU-DO-Makerspace/ns_theme_cleanblog/",
  "authors": [
    {
      "name": "Patrick Pedersen",
      "email": "ctx.xda@gmail.com",
      "role": "Developer",
      "homepage": "https://github.com/CTXz"
    },
    {
        "name": "Team NITSAN",
        "email": "info@nitsan.in",
        "role": "Developer",
        "homepage": "http://www.nitsan.in/"
    }
  ],
  "support": {
    "issues": "https://github.com/TU-DO-Makerspace/ns_theme_cleanblog/issues"
  },
  "license": "MIT",
  "require": {
    "typo3/cms-core": "^10.0  || ^11.0 ",
    "georgringer/news": "^9.0.0",
    "nitsan/ns-basetheme": ">=10.0.0",
    "nitsan/ns-news-comments": "^5.0.0"
  },
  "replace": {
      "typo3-ter/ns-theme-cleanblog": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_theme_cleanblog"
    }
  }
}
